{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Super Translator powered by GPT",
  "short_name": "Super Translator",
  "description": "Translate anything to anything!",
  "category": ["utilities", "productivity"],
  "start_url": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#333",
  "background_color": "#333",
  "icons": [
    {
      "src": "/img/favicon-180.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/img/favicon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/img/favicon-splash.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/img/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/img/maskable-icon.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/img/maskable-icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "share_target": {
    "action": "/via-share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "text": "text"
    }
  }
}
